<!DOCTYPE html>
<html lang='en'>

<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        <div class="content text-left">


          <%= form_for(@task) do |f| %>
            <%= f.label :name %>
            <%= f.text_field :name %>
            <%= f.label :due %>
            <%= f.text_field :due %>
            <%= f.submit %>
          <% end %>


        </div>
      </div>
    </div>
  </div>
</div>


</html>
